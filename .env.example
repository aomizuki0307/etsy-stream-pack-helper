# Copy to .env and fill values. Never commit real secrets.
GEMINI_API_KEY=your_api_key_here

# Base directory to store packs (default: ./packs). Accepts absolute paths.
# Example (Windows secondary SSD):
# STREAM_PACK_ROOT=D:\streampack_images
STREAM_PACK_ROOT=

# Preferred image models in priority order (comma-separated).
# Default: gemini-3-pro-image-preview,gemini-2.5-flash-image
GEMINI_IMAGE_MODELS=
